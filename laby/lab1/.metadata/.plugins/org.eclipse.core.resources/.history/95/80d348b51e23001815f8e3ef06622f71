subroutine s1 (a,b)
    implicit none
    integer :: a, b
    a = a*2
    b = b*2
end subroutine

function f1 (a,b) result (val)
    implicit none
    integer (kind=4), intent(in) :: a, b
    integer :: val
    val = a*b
end function

program main
    integer :: a = 1, b = 2
    call s1(a,b)
    !write (*,*) a, b
    a = f1 (a, b)
    write (*,*) a
end program main
